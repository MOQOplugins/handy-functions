{
    "name": "moqobe/handy-functions",
    "description": "A kickstart plugin for your craft project where you can use some of the most handy functions that are easy accessible with this plugin",
    "type": "craft-plugin",
    "version": "dev-master",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "handy functions"
    ],
    "support": {
        "docs": "https://github.com/MOQOplugins/handy-functions/blob/master/README.md",
        "issues": "https://github.com/MOQOplugins/handy-functions/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "MOQOplugins",
            "homepage": "https://www.github.com/MOQOplugins"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "moqobe\\handyfunctions\\": "src/"
        }
    },
    "extra": {
        "name": "Handy functions",
        "handle": "handy-functions",
        "developer": "MOQOplugins",
        "developerUrl": "https://www.github.com/MOQOplugins",
        "documentationUrl": "https://github.com/MOQOplugins/handy-functions/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/MOQOplugins/handy-functions/master/CHANGELOG.md",
        "class": "moqobe\\handyfunctions\\HandyFunctions"
    }
}
